{
  "name": "svelte-jsoneditor",
  "description": "A web-based tool to view, edit, format, transform, and validate JSON",
  "version": "0.0.8",
  "svelte": "dist/jsoneditor.mjs",
  "module": "dist/jsoneditor.mjs",
  "main": "dist/jsoneditor.js",
  "browser": "dist/jsoneditor.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/josdejong/svelte-jsoneditor.git"
  },
  "license": "ISC",
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "test": "mocha src/**/*.test.js",
    "lint": "eslint --cache src/**/*.svelte src/**/*.js tools/**/*.js *.js --ignore-pattern src/generated --ignore-pattern examples",
    "postinstall": "cd tools && node generateEmbeddedAceJsonWorker.js && cd ..",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "ace-builds": "^1.4.12",
    "ajv": "^7.0.4",
    "classnames": "^2.2.6",
    "debug": "^4.3.1",
    "diff-sequences": "^26.6.2",
    "immutable-json-patch": "^1.1.1",
    "jsonrepair": "^2.0.0",
    "lodash-es": "^4.17.20",
    "natural-compare-lite": "^1.4.0",
    "svelte-awesome": "^2.3.0",
    "svelte-select": "^3.16.1",
    "svelte-simple-modal": "^0.8.0",
    "sveltip": "^0.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.1.1",
    "eslint": "7.19.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-svelte3": "3.0.0",
    "mocha": "8.2.1",
    "rollup": "2.38.5",
    "rollup-plugin-svelte": "6.1.1",
    "rollup-plugin-terser": "7.0.2",
    "sass": "1.32.6",
    "svelte": "3.32.1",
    "svelte-preprocess": "4.6.6"
  },
  "keywords": [
    "svelte"
  ],
  "files": [
    "dist",
    "README.md"
  ]
}
